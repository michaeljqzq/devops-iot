{
  "assets": {
    "ARMResourceGroup": {
      "type": "endpoint:AzureRM",
      "inputs": {
        "scope": "/subscriptions/{{{inputs.subscriptionId}}}/resourceGroups/{{{inputs.resourceGroup}}}",
        "subscriptionId": "{{inputs.subscriptionId}}",
        "tenantId": "{{inputs.tenantId}}",
        "accessToken": "{{{inputs.accessToken}}}"
      }
    }
  }
}